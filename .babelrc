{
  "presets": [
    "react",
    "es2015",
    "stage-1"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-decorators",
    "transform-runtime",
    "syntax-async-functions",
    "transform-regenerator",
    "transform-object-assign"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  }
}
